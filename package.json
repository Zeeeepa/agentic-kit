{
  "name": "agentic-kit",
  "version": "1.0.0",
  "description": "Comprehensive toolkit for AI-driven software development with 13 specialized agents and 8+ powerful skills",
  "main": ".claude-plugin/plugin-standard.json",
  "keywords": [
    "agents",
    "skills",
    "ai-agents",
    "workflow-automation",
    "software-development",
    "product-management",
    "team-coordination",
    "project-management",
    "claude",
    "claude-code",
    "claude-plugin",
    "marketplace-plugin"
  ],
  "author": "Anthropic",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/anthropics/agentic-toolkit"
  },
  "bugs": {
    "url": "https://github.com/anthropics/agentic-toolkit/issues"
  },
  "homepage": "https://github.com/anthropics/agentic-toolkit#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    ".claude-plugin/",
    "agents/",
    "skills/",
    "hooks/",
    "resources/",
    "README.md",
    "QUICK-START.md",
    "AGENTS.md",
    "SKILLS.md",
    "ARCHITECTURE.md",
    "CONTRIBUTING.md",
    "TROUBLESHOOTING.md",
    "VARIANTS.md",
    "MARKETPLACE-DESCRIPTION.md",
    "MARKETPLACE-KEYWORDS.txt",
    "MARKETPLACE-ICON.md"
  ],
  "scripts": {
    "validate": "node -e \"require('fs').readFileSync('./.claude-plugin/plugin-standard.json', 'utf8'); console.log('Plugin manifest valid')\"",
    "lint": "echo 'Linting configuration files...' && npm run validate",
    "test": "echo 'Running test suite...' && npm run lint"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "variants": [
    {
      "name": "agentic-kit-lite",
      "version": "1.0.0",
      "description": "Lite variant with 3 core agents and no skills for minimal dependencies",
      "manifest": ".claude-plugin/plugin-lite.json"
    },
    {
      "name": "agentic-kit",
      "version": "1.0.0",
      "description": "Standard variant with all 13 agents and 8 core production skills",
      "manifest": ".claude-plugin/plugin-standard.json"
    },
    {
      "name": "agentic-kit-pro",
      "version": "1.0.0",
      "description": "Pro variant with all 13 agents and all 16 skills for maximum capabilities",
      "manifest": ".claude-plugin/plugin-pro.json"
    }
  ]
}
